<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.EstudianteController">
   <center>
      <VBox alignment="CENTER" prefHeight="367.0" prefWidth="600.0" spacing="10.0">
         <Label text="Registro de Estudiantes">
            <font>
               <Font name="Cooper Black" size="29.0" />
            </font></Label>
         <Button mnemonicParsing="false" onAction="#showMenu" text="Home" />
         <Button onAction="#showBox" text="Boxeo" />
         <Button mnemonicParsing="false" onAction="#showMuay" text="MuayThai" />
         <Button onAction="#ShowTaek" text="Taekwondo" />
         <Button onAction="#showJudo" text="Judo" />
         <Label fx:id="mensajeLabel" />
      </VBox>
   </center>
</BorderPane>
